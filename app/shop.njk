<body>

  {% include 'app/module/_header/_header.html' %}

  <main>

    <section class="top">
      <div class="container">
        <h2 class="top__title">
          Product list 1
        </h2>
        <div class="breadcrumbs">
          <ul class="breadcrumbs__list">
            <li class="breadcrumbs__item">
              <a href="" class="breadcrumbs__link">Home</a>
            </li>
            <li class="breadcrumbs__item">
              <a href="" class="breadcrumbs__link">Product list 1</a>
            </li>
          </ul>
        </div>
      </div>
    </section>

    <section class="shop">
      <div class="shop__container">
        <div class="shop__inner">
          <div class="shop-filters filter">

            <div class="filter-category filter__item">
              <h3 class="filter__title">
                CATEGORY
              </h3>
              <form class="filter-category__form" action="#">
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox" checked>
                  <div class="filter-category__checkbox">
                    <span>Chair</span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Table</span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Lamp</span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Watch</span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Clock</span>
                  </div>
                </label>
              </form>
            </div>

            <div class="filter-price filter__item">
              <h3 class="filter__title">PRICE</h3>
              <form class="filter-price__form" action="#">
                <input class="filter-price__input" type="text" data-min="100" data-max="500" data-from="200"
                  data-to="350">
                <label class="filter-price__label">

                  <div class="filter-price__wrapper">
                    $<span class="filter-price__from"></span>
                  </div>
                  <div class="filter-price__wrapper">
                    $<span class="filter-price__to"></span>
                  </div>
                </label>
              </form>
            </div>

            <div class="filter-brand filter__item">
              <h3 class="filter__title">
                BRAND
              </h3>
              <form class="filter-brand__form" action="#">
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox" checked>
                  <div class="filter-brand__checkbox">
                    <span>RFL</span>
                    <span class="filter-brand__checkbox-num">(10)</span>
                  </div>
                </label>
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox">
                  <div class="filter-brand__checkbox">
                    <span>Brothers</span>
                    <span class="filter-brand__checkbox-num">(21)</span>
                  </div>
                </label>
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox">
                  <div class="filter-brand__checkbox">
                    <span>Regal</span>
                    <span class="filter-brand__checkbox-num">(3)</span>
                  </div>
                </label>
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox">
                  <div class="filter-brand__checkbox">
                    <span>Nadia</span>
                    <span class="filter-brand__checkbox-num">(14)</span>
                  </div>
                </label>
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox">
                  <div class="filter-brand__checkbox">
                    <span>Otobi</span>
                    <span class="filter-brand__checkbox-num">(6)</span>
                  </div>
                </label>
              </form>
            </div>

            <div class="filter-products filter__item">
              <h3 class="filter__title">
                RECENT PRODUCTS
              </h3>
              <div class="filter-products__item">
                <a class="filter-products__link" href="#">
                  <div class="filter-products__img-box">
                    <img class="filter-products__img" src="images/filters/filter-products/1.png"
                      alt="filter product images">
                  </div>
                  <div class="filter-products__box">
                    <h6 class="filter-products__title">
                      Pendant lamp
                    </h6>
                    <div class="star" data-rateyo-rating="4"></div>
                    <div class="filter-products__price">
                      23$
                    </div>
                  </div>
                </a>
              </div>

              <div class="filter-products__item">
                <a class="filter-products__link" href="#">
